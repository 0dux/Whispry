
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id String @id @default(uuid())
  name String 
  email String @unique
  password String?
  profilePicture String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
